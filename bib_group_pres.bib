  author = {{P{\'e}rez-Cutillas}, Pedro and {P{\'e}rez-Navarro}, Alberto and {Conesa-Garc{\'i}a}, Carmelo and Zema, Demetrio Antonio and {Amado-{\'A}lvarez}, Jes{\'u}s Pilar},
  year = {2023},
