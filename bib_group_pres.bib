@article{dasariApplicationCbandSentinel1A2022,
  title = {Application of {{C-band}} Sentinel-{{1A SAR}} Data as Proxies for Detecting Oil Spills of {{Chennai}}, {{East Coast}} of {{India}}},
  author = {Dasari, Kiran and Anjaneyulu, Lokam and Nadimikeri, Jayaraju},
  year = {2022},
